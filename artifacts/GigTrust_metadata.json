		"devdoc": {
			"details": "A smart contract to manage a trust-based gig economy system using escrow for payment and a one-sided reputation system (Contractor rates Worker). * This contract is the definitive, immutable source of truth for all gig states and handles all ETH transactions securely on the blockchain.",
			"kind": "dev",
			"methods": {
				"acceptGig(uint256)": {
					"details": "Worker accepts an open gig."
				},
				"completeGig(uint256)": {
					"details": "Worker marks the gig as completed."
				},
				"confirmGigAndPay(uint256)": {
					"details": "Contractor confirms the gig is complete and pays the worker (releases escrow). This is the core money transfer logic."
				},
				"createGig(string,uint256)": {
					"details": "Contractor creates a new gig and sends the payment to the contract (escrow). The `payable` keyword is essential for receiving the ETH."
				},
				"getGigStatus(uint256)": {
					"details": "Retrieves the current status of a gig."
				},
				"getUserReputation(address)": {
					"details": "Calculates the average reputation score for a user."
				},
				"rateUser(uint256,address,uint256)": {
					"details": "Allows only the Contractor to rate the Worker after a gig is paid."
				},
				"registerUser(uint8)": {
					"details": "Allows a user to register as either a Contractor or a Worker. This function is typically called by the frontend or an authentication service."
				}
			},
